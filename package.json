{
  "name": "thebe",
  "version": "0.5.1",
  "description": "Thebe: turn static HTML pages into live documents",
  "main": "lib/index.js",
  "scripts": {
    "build": "webpack --mode development --display-chunks",
    "build:prod": "webpack --mode production --display-chunks",
    "build:json": "webpack --profile --json > webpack.stats.json",
    "build:watch": "webpack --mode development --progress --colors --watch",
    "prepare": "npm dedupe && npm run build:prod",
    "fmt": "prettier --trailing-comma=es5 --write *.js src/*.js examples/*.js test/*.js",
    "serve": "http-server -a 127.0.0.1 -o development.html",
    "develop": "concurrently \"npm run build:watch\" \"npm run serve\"",
    "test": "karma start --single-run --browsers ChromeHeadless karma.conf.js",
    "test:watch": "karma start --auto-watch --browsers ChromeHeadless karma.conf.js"
  },
  "files": [
    "lib/**",
    "src/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/executablebooks/thebe.git"
  },
  "keywords": [
    "jupyter",
    "notebook"
  ],
  "author": "Executable Books Project",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/executablebooks/thebe/issues"
  },
  "engines": {
    "node": ">=6",
    "npm": ">=4"
  },
  "homepage": "https://thebe.readthedocs.io/en/latest",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@jupyter-widgets/base": "^2.0.2",
    "@jupyter-widgets/controls": "^1.5.3",
    "@jupyter-widgets/html-manager": "^0.18.4",
    "@jupyter-widgets/output": "^2.0.1",
    "@jupyterlab/codemirror": "^1.2.8",
    "@jupyterlab/mathjax2": "^1.2.0",
    "@jupyterlab/outputarea": "^1.0.1",
    "@jupyterlab/services": "^4.2.2",
    "@jupyterlab/testutils": "^2.2.6",
    "@jupyterlab/theme-light-extension": "^1.2.8",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "codecov": "^3.8.0",
    "concurrently": "^5.3.0",
    "core-js": "^3.6.5",
    "css-loader": "^3.5.3",
    "file-loader": "^6.1.1",
    "http-server": "^0.12.3",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jquery": "^3.5.1",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "^4.0.2",
    "mocha": "^8.2.0",
    "mocha-loader": "^5.1.5",
    "prettier": "^2.1.2",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.11"
  }
}
